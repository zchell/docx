{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/client/$1"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30,
      "includeFiles": "client/public/Word_Free_1Year_Setup.msi"
    }
  },
  "env": {
    "NODE_ENV": "production"
  }
}